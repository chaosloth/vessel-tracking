{
  "name": "vessel-tracking",
  "version": "0.0.1",
  "author": "Christopher Connolly",
  "description": "Serverless backend for Vessel Tracking",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "build": "next build && npm run transfer-assets",
    "transfer-assets": "rm -rfv ../serverless/dist/assets/ && mkdir ../serverless/dist/assets/ && cp -R ./out/* ../serverless/dist/assets/"
  },
  "dependencies": {
    "react": "^18",
    "react-dom": "^18",
    "next": "14.2.2",
    "@twilio-paste/core": "^20.12.0",
    "@twilio-paste/icons": "^12.4.0"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.2"
  }
}
